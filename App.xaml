<Application x:Class="HelicopkkiDev.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:HelicopkkiDev"
             xmlns:vm ="clr-namespace:HelicopkkiDev.ViewModel"
             xmlns:v="clr-namespace:HelicopkkiDev.View"
             xmlns:m="clr-namespace:HelicopkkiDev.Model"
             xmlns:converter="clr-namespace:HelicopkkiDev.View.Converter"
             xmlns:materialDesign="http://materialdesigninxaml.net/winfx/xaml/themes"
             xmlns:math="http://hexinnovation.com/math"
             StartupUri="View/CharacterWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <math:MathConverter x:Key="MathConverter"/>
            <ResourceDictionary.MergedDictionaries>
                <materialDesign:BundledTheme BaseTheme="Light" PrimaryColor="DeepPurple" SecondaryColor="Lime" />
                <ResourceDictionary Source="pack://application:,,,/MaterialDesignThemes.Wpf;component/Themes/MaterialDesignTheme.Defaults.xaml" />
                <ResourceDictionary Source="View/Style/CustomMaterialStyle.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
